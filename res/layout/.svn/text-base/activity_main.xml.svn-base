<LinearLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mainbg2"
    android:orientation="vertical"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context=".MainActivity" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:background="#4f6d5e" >

        <ImageView
            android:id="@+id/isSolved"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:src="@drawable/toplogo" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <RelativeLayout
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="8dp"
                android:layout_marginRight="14dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/buttonunfocused" >

                <TextView
                    android:id="@+id/textView123"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="登录"
                    android:textColor="#FFFF" />
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@drawable/topbarline1" >
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <ScrollView
            android:id="@+id/scrollView1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="false"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="false"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <LinearLayout
                            android:id="@+id/news"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_news" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="新闻中心"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/sportsCenter"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_sports" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="健身卡"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/library"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_admissions" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="图书馆"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <LinearLayout
                            android:id="@+id/emptyClassroom"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_push_alert" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="空闲教室"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/schedules"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_events" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="课程表"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/swjtuMap"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_maps" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="交大地图"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <LinearLayout
                            android:id="@+id/pppoeActivity"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_directory" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="校园通讯录"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/swjtuChat"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_dining" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="生活服务"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/individualCenter"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_friends" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="个人中心"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <LinearLayout
                            android:id="@+id/secondClass"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_rss_feed11" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="空闲教室"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/wait"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_web13" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="课程表"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/wait"
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/balloon_white"
                            android:orientation="vertical"
                            android:paddingBottom="0dp"
                            android:paddingLeft="0dp"
                            android:paddingRight="0dp"
                            android:paddingTop="0dp" >

                            <ImageView
                                android:layout_width="60dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="center"
                                android:layout_margin="10dp"
                                android:layout_weight="1"
                                android:src="@drawable/icon_jobs" />

                            <TextView
                                android:id="@+id/SecondClassId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="5dp"
                                android:gravity="center"
                                android:text="交大地图"
                                android:textColor="@color/black_overlay" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerInParent="false"
            android:layout_gravity="bottom"
            android:background="#4f6d5e"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:background="#4f6d5e"
                android:gravity="bottom" >

                <ImageView
                    android:id="@+id/library"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="0.26"
                    android:src="@drawable/share" />

                <RelativeLayout
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal" >
                </RelativeLayout>

                <ImageView
                    android:id="@+id/library"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="10dp"
                    android:src="@drawable/right_arrow" />
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>

</LinearLayout>