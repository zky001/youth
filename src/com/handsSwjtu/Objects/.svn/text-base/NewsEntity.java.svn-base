package com.handsSwjtu.Objects;

import java.util.ArrayList;

import android.R.bool;
import android.graphics.Bitmap;

public class NewsEntity {

	private String newsTitle;
	private ArrayList<Bitmap> newsImg;
	private String newsContent;
	private boolean isWebPage;
	private boolean haveImg;


	public boolean isWebPage() {
		return isWebPage;
	}
	public void setIswebPage(boolean iswebPage) {
		this.isWebPage = iswebPage;
	}
	public boolean isHaveImg() {
		return haveImg;
	}
	public void setHaveImg(boolean haveImg) {
		this.haveImg = haveImg;
	}
	public String getNewsTitle() {
		return newsTitle;
	}
	public void setNewsTitle(String newsTitle) {
		this.newsTitle = newsTitle;
	}
	public ArrayList<Bitmap> getNewsImg() {
		return newsImg;
	}
	public void setNewsImg(ArrayList<Bitmap> newsImg) {
		this.newsImg = newsImg;
	}
	public String getNewsContent() {
		return newsContent;
	}
	public void setNewsContent(String newsContent) {
		this.newsContent = newsContent;
	}
	
	
}
